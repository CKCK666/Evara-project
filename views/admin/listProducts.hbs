<div class="card-body">
    <div class="table-responsive">
         {{#if products}}
        <table class="table table-hover">
            <thead>
                <tr>
                     <th>Sl.No</th>
                    <th>Image</th>
                    <th>Name</th>
                    <th>Price</th>
                    <th>Status</th>
                    <th>Date</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>
                
               
                {{#each products}}
                <tr>
                    <td>{{index}}</td>
                    <td><img src="{{mainProductUrl}}" class="img-sm img-thumbnail" alt="Item"></td>
                    <td><a href="#">{{strProductName}}</a></td>
                    <td>{{intPrice}}</td>
                    <td><span id="statusId" class="badge rounded-pill {{#eq strStatus 'Blocked'}}alert-warning{{else}}{{#eq strStatus 'Active'}}alert-success{{else}}{{/eq}}{{/eq}}">{{strStatus}}</span></td>
                    <td>{{createdDate}}</td>
                    <td>

                        <a href="/admin/getProductEdit?pkProductId={{pkProductId}}" class="btn btn-md rounded font-sm">
                            <i class="fas fa-edit"></i> Edit
                        </a>
                       
                         <a href="#" onclick="handleBlock('{{pkProductId}}','{{strStatus}}','product')" class="btn btn-md rounded font-sm" style="width: 95px;">
                            <i class="{{#eq strStatus 'Blocked'}}fas fa-check-circle{{else}}fas fa-ban{{/eq}}"></i>{{#eq strStatus 'Blocked'}}Unblock{{else}}Block{{/eq}}
                        </a>
                        <a href="#" onclick="handleDelete('{{pkProductId}}', 'product')" class="btn btn-md rounded font-sm">
                            <i class="fas fa-trash-alt"></i> Delete
                        </a>
                       
                    </td>
                </tr>
                
                {{/each}}
             
    
              

                <!-- Add more rows as needed -->
        
        </table>
        {{else}}
        <h1 class="text-center">No Data Found!!!!!</h1>
          {{/if}}

    </div>
</div>

