  <div class="page-header breadcrumb-wrap">
            <div class="container">
                <div class="breadcrumb">
                    <a href="/" rel="nofollow">Home</a>
                    <span></span> Shop
                    <span></span> Your Cart
                </div>         
            </div>
        </div>
        <section class="mt-50 mb-50">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="table-responsive">
                            <table class="table shopping-summery text-center clean">
                                <thead>
                                    <tr class="main-heading">
                                        <th scope="col">Image</th>
                                        <th scope="col">Name</th>
                                        <th scope="col">Price</th>
                                        <th scope="col">Quantity</th>
                                        <th scope="col">Subtotal</th>
                                        <th scope="col">Remove</th>
                                    </tr>
                                </thead>
                                <tbody>
                                   
                                    {{#if cartProducts}}
                                    {{#each cartProducts}}
                                    <tr class="cart-product-details">
                                        <input type="hidden" id="pkProductId" value={{pkProductId}}>
                                        <input type="hidden" id="intStock" value={{intStock}}>
                                        <td class="image product-thumbnail"><img src={{mainProductUrl}} alt="#"></td>
                                        <td class="product-des product-name">
                                            <h5 class="product-name"><a href="shop-product-right.html">{{strProductName}}</a></h5>
                                            <p class="font-xs">{{strDescription}}<br> .
                                            </p>
                                        </td>
                                        <td class="price" data-title="Price"><span>{{intPrice}}</span></td>
                                        <td class="text-center" data-title="Stock">
                                            <div class="detail-qty border radius  m-auto">
                                                {{!-- <a onclick="handleQuantity('-1','{{pkProductId}}')" href="#" class="qty-down"><i class="fi-rs-angle-small-down"></i></a>
                                                <span id="qty-val" class="qty-val">{{intQuantity}}</span>
                                                <a href="#" onclick="handleQuantity('1','{{pkProductId}}')" class="qty-up"><i class="fi-rs-angle-small-up"></i></a> --}}
                                                  <a  href="#" class="qty-down"><i class="fi-rs-angle-small-down"></i></a>
                                              <span id="qty-val" class="qty-val">{{intQuantity}}</span>
                                                <a href="#"  class="qty-up"><i class="fi-rs-angle-small-up"></i></a>
                                            </div>
                                        </td>
                                        <td class="text-right" data-title="Cart">
                                            <span class="product-total-amount">{{intTotalPrice}}</span>
                                        </td>
                                        <td class="action" data-title="Remove"><a href="#" class="text-muted"><i class="fi-rs-trash"></i></a></td>
                                    </tr>
                                    {{/each}}
                                     {{/if}}
                                   
                                    <tr>
                                        <td colspan="6" class="text-end">
                                            <a href="#" class="text-muted"> <i class="fi-rs-cross-small"></i> Clear Cart</a>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                        <div class="cart-action text-end">
                          
                        </div>
                        <div class="divider center_icon mt-50 mb-50"><i class="fi-rs-fingerprint"></i></div>
                        <div class="row mb-50">
                            <div class="col-lg-6 col-md-12">
                            
                            </div>
                            <div class="col-lg-6 col-md-12">
                                <div class="border p-md-4 p-30 border-radius cart-totals">
                                    <div class="heading_s1 mb-3">
                                        <h4>Cart Totals</h4>
                                    </div>
                                    <div class="table-responsive">
                                        <table class="table">
                                            <tbody>
                                                {{#if cartDetails}}
                                                {{#each cartDetails}}
                                                <tr>
                                                    <td class="cart_total_label">Cart Subtotal</td>
                                                    <td class="cart_total_amount"><span id="cart_subtotal" class="font-lg fw-900 text-brand">{{total_cart_price}}</span></td>
                                                </tr>
                                                <tr>
                                                    <td class="cart_total_label">Shipping</td>
                                                    <td class="cart_total_amount"> <i class="ti-gift mr-5"></i> Free Shipping</td>
                                                </tr>
                                                <tr>
                                                    <td class="cart_total_label">Total</td>
                                                    <td class="cart_total_amount"><strong><span id="totalPrice" class="font-xl fw-900 text-brand">{{total_cart_price}}</span></strong></td>
                                                </tr>
                                                {{/each}}
                                                {{/if}}
                                            </tbody>
                                        </table>
                                    </div>
                                    <a href="/getCheckoutPage" class="btn "> <i class="fi-rs-box-alt mr-10"></i> Proceed To CheckOut</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>